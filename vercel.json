{
    "version": 2,
    "builds": [
      {
        "src": "routes/**/*.ts",
        "use": "@vercel/node"
      },
      {
        "src": "index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/routes/(.*)",
        "dest": "/routes/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.ts"
      }
    ]
  }
  